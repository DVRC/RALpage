% Test gray rendering
%%%%%% 2 0 {dup mul exch dup mul add 1 exch sub} bind setscreen
10 0 { exch pop abs 1 exch sub } bind setscreen
59 59 scale
/N 10 def
/D 1.3 def
0 1 N 1 sub
 { /i exch def
   0 1 N 1 sub
    { /j exch def
      i N mul j add N N mul 1 sub div setgray newpath
      j i moveto 0 D rlineto D 0 rlineto 0 D neg rlineto
      closepath fill
    } for
 } for
/tworects
 { 0.5 setgray newpath
   0 N moveto 0 1 rlineto N 0 rlineto 0 -1 rlineto closepath fill
   0.3 setgray newpath
   0 N 1 add moveto 0 1 rlineto N 0 rlineto 0 -1 rlineto closepath fill
 } def
tworects
[0 1 1 0 0 0] concat		% exchange x and y
tworects

showpage
