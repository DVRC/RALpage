%/setworldscale {
%  /world 
%    xmin neg ymax neg                   % translate from upper left corner
%    matrix translate                    % to origin
%
%    72 pagewidth mul 6 sub xmax xmin sub div
%    72 pageheight mul 6 sub ymax ymin sub div   % change scale from points
%    matrix scale                                % to world coordinates
%
%    pagex 72 mul 3 add
%    11 pagey sub 72 mul 3 sub         % move origin to ul corner of window
%    matrix translate 
%    % the above puts 3 matricies on the stack.  Now concat them
%    matrix concatmatrix             % concat last 2 transforms
%    matrix concatmatrix             % concat first trans. and prev. concat.
%  def
%} def
%
/setpixelscale {
  /pixel
    72 pagewidth mul 6 sub pixwidth 1 sub div
    72 pageheight mul 6 sub pixheight 1 sub neg div
    matrix scale

    pagex 72 mul 3 add 11 pagey sub 72 mul 3 sub
    matrix translate
    matrix concatmatrix
  def
} def

/setworldscale {
  /world
    72 pagewidth mul 6 sub xmax xmin sub div
    72 pageheight mul 6 sub ymax ymin sub div   % change scale from points
    matrix scale                                % to world coordinates

    pixwidth xmax xmin sub div xmin neg mul    % move to pixel origin
    pixheight ymax ymin sub div ymax mul
    pixel transform matrix translate
    matrix concatmatrix
  def
} def

/box {
    pagex 72 mul 11 pagey sub 72 mul moveto
    pagewidth 72 mul 0 rlineto
    0 pageheight 72 mul neg rlineto
    pagewidth 72 mul neg 0 rlineto
    closepath stroke
} def

/axis {
    xmax 0 world transform moveto xmin 0 world transform lineto
    0 ymax world transform moveto 0 ymin world transform lineto stroke
} def

/setclip {
    initclip xmin ymin world transform moveto 
    xmax ymin world transform lineto
    xmax ymax world transform lineto 
    xmin ymax world transform lineto
    closepath clip newpath
} def

/drawsquare {
  moveto         % move to center
  squaresize 2 div dup rmoveto    % move to upper right corner
  squaresize neg 0 rlineto
  0 squaresize neg rlineto
  squaresize 0 rlineto
  closepath stroke } def
/drawdiamond {
  moveto
  0 diamondside rmoveto
  diamondside neg dup rlineto
  diamondside dup neg rlineto
  diamondside dup rlineto
  closepath stroke} def
/drawtriangle {
  moveto
  0 triangleheight 2 div rmoveto
  trianglesize 2 div neg triangleheight neg rlineto
  trianglesize 0 rlineto
  closepath stroke} def
/drawcross {
  moveto
  0 crosssize 2 div rmoveto
  0 crosssize neg rlineto
  crosssize 2 div dup rmoveto
  crosssize neg 0 rlineto
  stroke} def
/drawstar {
  moveto
  0 starsize 2 div rmoveto
  0 starsize neg rlineto
  starsize 2 div dup rmoveto
  starsize neg 0 rlineto
  starsize 2 div 0 rmoveto
  stardiag 2 div  dup rmoveto
  stardiag neg dup rlineto
  0 stardiag rmoveto
  stardiag dup neg rlineto
  stroke} def
/drawdot {
  dotsize 2 div 0 360 arc 
  fill} def
/drawcircle {
  circlesize 2 div 0 360 arc
  stroke} def
/drawbar {
  /yval exch def
  /xval exch def
  xval halfbar sub 0 worldtopix moveto
  xval halfbar sub yval worldtopix lineto
  xval halfbar add yval worldtopix lineto
  xval halfbar add 0 worldtopix lineto
  stroke} def
/drawimpulse {
  2 copy 
  worldtopix moveto
  pop 0 worldtopix lineto
  stroke} def
/pixline {pixel transform moveto pixel transform lineto stroke} def
/pixdot {pixel transform drawdot} def
/pixtic {pixel transform moveto show} def
/pixrect {dup dup dup dup dup dup dup dup dup
          0 get exch 1 get pixel transform moveto
          2 get exch 1 get pixel transform lineto
          2 get exch 3 get pixel transform lineto
          0 get exch 3 get pixel transform lineto
          0 get exch 1 get pixel transform lineto} def
/connect {dup 0 get exch 1 get worldtopix lineto} def
/square {dup 0 get exch 1 get worldtopix drawsquare} def
/triangle {dup 0 get exch 1 get worldtopix drawtriangle} def
/circle {dup 0 get exch 1 get worldtopix drawcircle} def
/cross {dup 0 get exch 1 get worldtopix drawcross} def
/dot {dup 0 get exch 1 get worldtopix drawdot} def
/diamond {dup 0 get exch 1 get worldtopix drawdiamond} def
/star {dup 0 get exch 1 get worldtopix drawstar} def
/impulse {dup dup 0 get exch 1 get worldtopix moveto
          0 get 0 worldtopix lineto} def
/bar {dup dup dup dup dup 0 get halfbar sub 0 worldtopix moveto
      0 get halfbar sub exch 1 get worldtopix lineto
      0 get halfbar add exch 1 get worldtopix lineto
      0 get halfbar add 0 worldtopix lineto
} def
/labimp {pixel transform moveto pixel transform lineto stroke} def
/worldtopix {
  world transform} def

