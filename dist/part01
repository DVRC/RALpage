X     0 0 moveto (T) show
X     0 1 dtransform
X      dup mul exch dup mul add
X      size gt
X         {
X          currentpoint translate
X          0 .1 translate 0.6 0.6 scale
X          0 0 moveto (R) show
X          currentpoint translate
X          .75 0 translate
X	  gsave
X	    90 rotate tree
X	  grestore
X	  .75 0 translate
X	  90 rotate -1 1 scale
X	  (T) stringwidth neg exch neg exch translate
X	  tree
X	  } if
X  } def
X
X100 400 translate 200 200 scale
X
Xtree
Xshowpage
X
END_OF_FILE
