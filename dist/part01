X * 
X *	Crispin Goswell @ Rutherford Appleton Laboratory caag@uk.ac.rl.vd
X */
X#include "main.h"
X
X#define SMALL_SIZE	4096
X
Xstatic char *small_buf;
Xstatic int left = 0;
X
Xchar *Malloc (u) unsigned u;
X {
X 	char *malloc (), *res;
X 	
X	if (0/*u < SMALL_SIZE*/)
X 	 {
X  	 	char *res;
X	 	if (left < u)
X	 	 {
X	 		small_buf = Malloc (SMALL_SIZE);
X	 		left = SMALL_SIZE;
X	 	 }
X 	 	res = small_buf;
X 	 	small_buf += u;
X 	 	left -= u;
X 	 	
X 	 	return res;
X 	 }
X 	
X 	PanicIf ((res = malloc (u)) == NULL, "malloc failed");
X 	return res;
X }
X
XFree (s) char *s;
X {
X	/* free (s);	*/
X }
END_OF_FILE
