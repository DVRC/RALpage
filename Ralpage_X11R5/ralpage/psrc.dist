/executive {
	{
		{ (\n) print
			prompt flush (%statementedit) (r) file
			dup status not {(\n) print quit} if
			cvx exec } stopped
		{ (stopped\n) print } if
	} loop
 } def

/bind {
	dup type /arraytype eq 1 index xcheck and {
    		dup length 0 1 3 2 roll 1 sub {
			2 copy get
			dup type /arraytype eq {
		    		bind pop pop
			} {
	 	   		dup where {
					exch get
					dup type /operatortype eq {
			    			2 index 2 index
			    			3 2 roll put
			    			pop
					} {
			    			pop pop 
					} ifelse
	  	 		} { 
					pop pop
	  	  		} ifelse
			} ifelse
   	 	} for
   	} if
} def

/concatstrings { % usage: array-of-strings --- string
	dup 0 exch { length add } forall string
	exch 0 exch { 3 copy putinterval length add } forall pop
} def

