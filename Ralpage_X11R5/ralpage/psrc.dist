/findfont {
	dup type /stringtype eq { cvn } if 
	dup systemdict exch /UnmappedName exch put
	dup FontMap exch known {
		FontMap exch get
	} if
	dup FontDirectory exch known
	{
		oldfindfont
	} {
		dup
		findfontdict begin
			dup systemdict exch /MappedName exch put
			FFBuf cvs [ exch font-library exch ] concatstrings
				{ run } stopped { pop pop /Serif-Roman } if
		end
		findfont
	} ifelse
} def

